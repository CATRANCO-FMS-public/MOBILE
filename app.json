{
  "expo": {
    "name": "TransitTrack-app",
    "slug": "TransitTrack-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/catranco_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/images/catranco_logo.png",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCh2BLbmWKz97jg9X-OTTTLf_DJzssw-W0"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/catranco_logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.khenpaler.TransitTrackapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8d4c47c4-2250-4929-abca-9b6a23bff2f4"
      }
    }
  }
}
