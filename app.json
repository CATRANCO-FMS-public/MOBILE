{
  "expo": {
    "splash": {
      "image": "./assets/images/splashscreen_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "name": "TransitTrack",
    "slug": "TransitTrack-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/catranco_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "icon": "./assets/images/catranco_app_icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/catranco_app_icon.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/images/catranco_app_icon.png",
        "color": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ],
      "package": "com.khenpaler.TransitTrackapp"
    },
    "plugins": [
      "react-native-background-fetch",
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/catranco_logo.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-background-fetch",
        {
          "minimumInterval": 15
        }
      ]
    ],
    "audio": {
      "mode": "media",
      "interruptionModeAndroid": "duckAudio",
      "staysActiveInBackground": true,
      "playsInSilentModeIOS": true
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b3860b2d-8cff-4a89-af19-34e9e7c0c422"
      }
    }
  }
}
