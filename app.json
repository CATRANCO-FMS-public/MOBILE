{
  "expo": {
    "name": "TransitTrack-app",
    "slug": "TransitTrack-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/catranco_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ],
        "NSPushNotificationsUsageDescription": "This app uses notifications to keep you informed about timer updates.",
        "NSUserTrackingUsageDescription": "This app uses tracking for notifications and analytics."
      }
    },
    "android": {
      "icon": "./assets/images/catranco_logo.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/catranco_logo.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/images/catranco_notification_icon.png",
        "color": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ],
      "package": "com.khenpaler.TransitTrackapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/catranco_logo.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-background-fetch",
        {
          "minimumInterval": 15
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "24bf3270-e8a1-424e-a97f-0e95568b0efd"
      }
    }
  }
}
